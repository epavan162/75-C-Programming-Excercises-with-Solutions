/*c program to print subjects in computer science using switch
PIN NO:18172-CM-002
E.PAVAN KALYAN*/
#include<stdio.h>
void main()
{
		int i;
	printf("Enter the value of i");
	scanf("%d",&i);
	switch(i)
	{
		case 2/2:printf("ENGLISH\n");
					break;
		case 4/2:printf("MATHEMATICS\n");
					break;
		case 6/2:printf("PHYSICS\n");
					break;
		case 8/2:printf("CHEMISTRY\n");
					break;
		case 10/2:printf("BASICS OF COMPUTER ENGINEERING\n");
					break;
		case 12/2:printf("C LANGUAGE\n");
					break;
		case 14/2:printf("ENGINEERING DRAWING\n");
					break;
		default:printf("SORRY WRONG INPUT\n");
					break;
	}
}
